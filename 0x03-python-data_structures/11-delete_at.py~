#!/usr/bin/python3
def delete_at(my_list=[], idx=0):
    if idx < 0 or idx > (len(my_list) - 1):
        return my_list

    for i in range(len(my_list)):
        if i < len(my_list):
            if idx == i:
                del my_list[i]
            else:
                my_list[i] = my_list[i]
    return my_list

my_list = [1, 2, 3, 4, 5, 6]
idx = 3
new_list = delete_at(my_list, idx)
print(new_list)
print(my_list)
